@model IEnumerable<Usuario>
    @{
        ViewData["Title"] = "Listagem de Usuário";
    }
    <h2>Listagem de Usuário</h2>
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nome</th>
                <th>Login</th>
                <th>Senha</th>                
            </tr>
        </thead>
        <tbody>
            @foreach ( Usuario u in Model)
            {
                <tr>
                    <td>@u.Id</td>
                    <td>@u.Nome</td>
                    <td>@u.Login</td>
                    <td>@u.Senha</td>
                </tr>
            }
        </tbody>
    </table>